{
    "manifest_version": 3,
    "name": "Prompt Builder",
    "version": "1.0",
    "icons": {
        "16": "/icon.png",
        "48": "/icon.png",
        "128": "/icon.png"
    },
    "action": {
      "default_title": "Prompt Builder"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "tabs"],
    "host_permissions": ["<all_urls>"]
  }
  